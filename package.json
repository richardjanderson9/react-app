{
  "name": "react-app",
  "version": "0.2.1",
  "private": true,
  "description": "A simple React application.",
  "author": {
    "name": "Richard Anderson"
  },
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^5.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.{js,css,json,md}",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "url",
    "url": "https://github.com/richardjanderson9/react-app/",
    "url2": "https://github.com/richardjanderson9/react-app/"
  },
  "bugs": {
    "url": "https://github.com/richardjanderson9/react-app/issues"
  },
  "homepage": "http://localhost:3000",
  "devDependencies": {
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3"
  },
  "engines": {
    "node": ">=22.16.0",
    "npm": ">=10.9.2"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "serialize-javascript": "^6.0.2",
    "inflight": "^1.0.6",
    "webpack-dev-server": "^5.2.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}